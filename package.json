{
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "js:build": "browserify ./src/scripts/app.jsx  -t [ babelify ] -t [ localenvify ] --extension=jsx -o ./bundle.js",
    "js:watch": "watchify ./src/scripts/app.jsx -d -t [ babelify ] -t [ localenvify ] --extension=jsx -o ./bundle.js",
    "build": "npm run js:build",
    "watch": "npm run js:watch",
    "start": "npm run build & npm run watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "wpapi": "^0.9.1"
  },
  "devDependencies": {
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "localenvify": "^1.0.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "underscore": "global:_"
  }
}
